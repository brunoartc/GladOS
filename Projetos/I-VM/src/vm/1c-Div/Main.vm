function Main.main 0
	push constant 15
	pop temp 1
	push constant 0
	pop temp 0
	label LOOP_START
		push temp 1
		push constant 1
		lt
		if-goto END

		push temp 1
		push constant 5
		sub
		pop temp 1

		push temp 0
		push constant 1
		add
		pop temp 0

		goto LOOP_START
	label END
